    </div>
    <div class="col s12 m6 l7">
        @await Html.PartialAsync("Chart/SrEvolution")
    </div>
</div>
<div class="row">
    <div class="col s12">
        @await Component.InvokeAsync(nameof(AverageSrChangeComponent))
    </div>
</div>