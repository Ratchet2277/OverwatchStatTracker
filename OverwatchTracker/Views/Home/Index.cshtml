@using WebApplication.Components

@{
    ViewData["Title"] = "Home Page";
}
<div class="row">
    <div class="col s12">
        @await Component.InvokeAsync(nameof(GameListComponent))
    </div>
</div>
<div class="row center">
    <div class="col s12 m6 l5">
        @await Html.PartialAsync("Chart/WinRate/ByType")
