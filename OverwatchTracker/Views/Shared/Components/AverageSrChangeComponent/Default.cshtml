@model Dictionary<DomainModel.Types.GameType, Tuple<float, float>>

<table class="responsive-table center">
    <thead>
    <tr>
        @foreach (var (gameType, _) in Model)
        {
            <th colspan="2" scope="colgroup">
                @gameType
            </th>
        }
    </tr>
    <tr>
        @foreach (var _ in Model)
        {
            <th scope="col">
                By win
            </th>
            <th scope="col">
                By lose
            </th>
        }
    </tr>
    </thead>
    <tbody>
    <tr>
        @foreach (var (_, (byWin, byLose)) in Model)
        {
            <td>+@Math.Round(byWin, 2)</td>
            <td>@Math.Round(byLose, 2)</td>
        }
    </tr>
    </tbody>
</table>